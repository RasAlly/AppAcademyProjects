<h1>Log in</h1>

<form action="<%= session_url %>" method="post">

    <label>username:
        <input type="text"
        name="user[username]"
        value="<%= @user.username %>">
    </label>

    <label>password:
        <input type="password"
        name="user[password]">
    </label>

    <input type="submit" value="login">


</form>